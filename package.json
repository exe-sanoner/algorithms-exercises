    "react-router-dom": "5.2.0",
    "xxhashjs": "0.2.2"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "jest": "^26.6.3",
    "parcel-bundler": "^1.12.3",
    "prettier": "^2.3.2"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  }
