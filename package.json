    "lint:check": "eslint --ignore-path ./.gitignore --quiet \"./**/*.{js,ts}\""
  },
  "dependencies": {
    "lodash": "4.17.21",
    "parcel": "2.0.0-beta.2",
    "react": "17.0.2",
